{
  "name": "codesendertelegram",
  "displayName": "CodeSenderTelegram",
  "publisher" : "needosleep",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/needosleep/CodeSenderTelegram.git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setChatId",
        "title": "Set Chat ID"
      },
      {
        "command" : "extension.resetChatId",
        "title": "Reset Chat Id"
      },
      {
        "command": "extension.sendToTelegram",
        "title": "Send to Telegram"
      }
    ],
    "keybindings": [
      {
        "command": "extension.sendToTelegram",
        "key": "ctrl+shift+/",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "CodeSenderTelegram",
      "properties": {
        "codesendertelegram.chatId": {
          "type": "string",
          "default": "",
          "description": "chat_id in telegram"
        }
      }
    },
    "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "vscode-test"
    },
    "devDependencies": {
      "@types/vscode": "^1.94.0",
      "@types/mocha": "^10.0.8",
      "@types/node": "20.x",
      "eslint": "^9.11.1",
      "@vscode/test-cli": "^0.0.10",
      "@vscode/test-electron": "^2.4.1"
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ]
}
